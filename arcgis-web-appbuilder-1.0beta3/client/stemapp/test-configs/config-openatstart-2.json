{
  "theme": {
    "name": "FoldableTheme"
  },
  "portalUrl": "http://www.arcgis.com",
  "appId":"senJpIJA845L4FlP",
  "title": "ArcGIS Web Application",
  "subtitle": "with ArcGIS WebApp Builder",
  "about": "widgets/About/Widget",
  "httpProxy": {"url": "/proxy.js"},
  
  "geometryService": "http://tasks.arcgisonline.com/ArcGIS/rest/services/Geometry/GeometryServer",
  
  "links":[
    {
      "label": "ArcGIS Online",
      "url": "//www.arcgis.com"
    }
  ],

  "widgetOnScreen": {
    "panel": {
      "uri": "themes/FoldableTheme/panels/TitlePanel/Panel"
    },
    "widgets": [{
        "uri": "themes/FoldableTheme/widgets/HeaderController/Widget",
        "positionRelativeTo": "browser",
        "position": {
          "left": 0,
          "top": 0,
          "right": 0,
          "height": 40
        }
      },{
        "label": "Select a basemap",
        "uri": "widgets/BasemapGallery/Widget",
        "position": {
          "left": 45,
          "top": 45,
          "width": 360,
          "height": 410
        }
      },{
        "label": "Measure",
        "uri": "widgets/Measurement/Widget",
        "position": {
          "left": 95,
          "top": 45,
          "height": 200
        },
        "openAtStart": true
      }, {
        "label": "Query",
        "uri": "widgets/Query/Widget",
        "position": {
          "left": 145,
          "top": 45,
          "width": 380,
          "height": 410
        },
        "openAtStart": true
      }]
  },

  "map": {
    "3D": false,
    "2D": true,

    "position": {
      "left": 0,
      "top": 40,
      "right": 0,
      "bottom": 0
    },

    "portalUrl": "http://www.arcgis.com",
    
    "itemId": "5b9a674043ca4ec783b7eb85037aefa8",

    "mapOptions": {
      "center": [-98, 40],
      "scale": 16937199
    }
  },

  "widgetPool": {
    "panel": {
      "uri": "themes/FoldableTheme/panels/FoldablePanel/Panel",
      "positionRelativeTo": "map",
      "position": {
        "top": 5,
        "right": 5,
        "bottom": 5
      }
    },


    "widgets": [{
      "label": "Bookmark",
      "uri": "widgets/Bookmark/Widget",
      "openAtStart": true
    },{
      "label": "Legend",
      "uri": "widgets/Legend/Widget",
      "openAtStart": true
    }, {
      "label": "LayerList",
      "uri": "widgets/LayerList/Widget"
    }]
  }
}